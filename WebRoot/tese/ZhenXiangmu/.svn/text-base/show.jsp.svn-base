<%@ page contentType="text/html;charset=GBK" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
    <meta http-equiv="Content-Type" content="text/html; charset=GBK">
    <link href="${pageContext.request.contextPath}/styles/default.css" rel="stylesheet" type="text/css" />  
    <script language="JavaScript" src="${pageContext.request.contextPath}/scripts/status.js"></script>  
<head>
</head>
<body>
<form action="${pageContext.request.contextPath}/tese/Zhen_xiangmu!edit.do" method="post">
	<input type="hidden" Id="id" name="id" value="${zhen_xiangmu.id}"/>
	<table width="100%" border="1" class="myInput">
    <tr>	
		<td colspan="4" class="myInputTitle"><strong>镇（乡）近期建设项目情况信息</strong></td>	
	</tr>	
	    <tr>
			<td width="15%" class="myInputTitle">地点</td>	
			<td width="35%">${zhen_xiangmu.unit_name}</td>
			<td width="15%" class="myInputTitle">项目分类</td>	
			<td width="35%">${zhen_xiangmu.fen_lei}</td>
	    </tr>
	    <tr>
			<td width="15%" class="myInputTitle">项目名称</td>	
			<td width="35%">${zhen_xiangmu.ming_cheng}</td>
			<td width="15%" class="myInputTitle">建设性质</td>	
			<td width="35%">${zhen_xiangmu.xing_zhi}</td>
	    </tr>
	    <tr>
			<td width="15%" class="myInputTitle">建设规模(平方米)</td>	
			<td width="35%">${zhen_xiangmu.gui_mo}</td>
			<td width="15%" class="myInputTitle">总投资（万元）</td>	
			<td width="35%">${zhen_xiangmu.zong_touzi}</td>
	    </tr>
	    <tr>
			<td width="15%" class="myInputTitle">建设年限（年）</td>	
			<td width="35%">${zhen_xiangmu.nian_xian}</td>
			<td width="15%" class="myInputTitle">类别
（用途）
</td>	
			<td width="35%">${zhen_xiangmu.lei_bie}</td>
	    </tr>
	    <tr>
			<td width="15%" class="myInputTitle">用地规模(平方米)</td>	
			<td width="35%">${zhen_xiangmu.yong_di}</td>
			<td width="15%" class="myInputTitle">用地使用年限（年）</td>	
			<td width="35%">${zhen_xiangmu.yongdi_nianxian}</td>
	    </tr>
	    <tr>
			<td width="15%" class="myInputTitle">新增建设用地规模(平方米)</td>	
			<td width="35%">${zhen_xiangmu.xinzeng_yongdi}</td>
			<td width="15%" class="myInputTitle">添加时间</td>	
			<td width="35%">${zhen_xiangmu.input}</td>
	    </tr>
     <tr>	
		<td colspan="4" align="center" style="height: 30px">
	      <input type="submit" value=" 编 辑 "/>&nbsp;&nbsp;
	      <input type="button" value="返回列表" onclick="window.location='${pageContext.request.contextPath}/tese/Zhen_xiangmu!list.do'"/>		
	    </td>	
	</tr>
	</table>
</form>
</body>
</html>