<%@ page contentType="text/html;charset=GBK" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
    <meta http-equiv="Content-Type" content="text/html; charset=GBK">
    <script language="JavaScript" src="${pageContext.request.contextPath}/scripts/status.js"></script>
    <link href="${pageContext.request.contextPath}/styles/default.css" rel="stylesheet" type="text/css" />    
<head>
</head>
<body>
<form action="${pageContext.request.contextPath}/admin/Economy!edit.do" method="post">
	<input type="submit" value=" 编 辑 "/>
	<input type="button" value="返回列表" onclick="window.location='${pageContext.request.contextPath}/admin/Economy!list.do'"/>

	<input type="hidden" Id="id" name="id" value="${economy.id}"/>
	<table width="100%" border="1" class="myInput">
	    <tr>
			<td width="20%" class="myInputTitle">对应用户主键</td>	
			<td width="30%">${economy.unit_id}</td>
			<td width="20%" class="myInputTitle">年份</td>	
			<td width="30%">${economy.year}</td>
	    </tr>
	    <tr>
			<td width="20%" class="myInputTitle">总人口（万人）</td>	
			<td width="30%">${economy.zong_ren}</td>
			<td width="20%" class="myInputTitle">农业人口（万人）</td>	
			<td width="30%">${economy.nong}</td>
	    </tr>
	    <tr>
			<td width="20%" class="myInputTitle">农户数（万户）</td>	
			<td width="30%">${economy.nong_hu}</td>
			<td width="20%" class="myInputTitle">农村劳动力（万人）</td>	
			<td width="30%">${economy.lao_dong}</td>
	    </tr>
	    <tr>
			<td width="20%" class="myInputTitle">总面积（万平方公里）</td>	
			<td width="30%">${economy.zong_mianj}</td>
			<td width="20%" class="myInputTitle">耕地面积（万平方公里）</td>	
			<td width="30%">${economy.geng_di}</td>
	    </tr>
	    <tr>
			<td width="20%" class="myInputTitle">全镇生产总值（亿元）</td>	
			<td width="30%">${economy.zong_shengc}</td>
			<td width="20%" class="myInputTitle">农村经济总收入（亿元）</td>	
			<td width="30%">${economy.jingj_zong}</td>
	    </tr>
	    <tr>
			<td width="20%" class="myInputTitle">农民人均纯收入（元）</td>	
			<td width="30%">${economy.nongm_jun}</td>
			<td width="20%" class="myInputTitle">第一产业总值（亿元）</td>	
			<td width="30%">${economy.yi}</td>
	    </tr>
	    <tr>
			<td width="20%" class="myInputTitle">第二产业总值（亿元）</td>	
			<td width="30%">${economy.er}</td>
			<td width="20%" class="myInputTitle">第三产业总值（亿元）</td>	
			<td width="30%">${economy.san}</td>
	    </tr>
	    <tr>
			<td width="20%" class="myInputTitle">乡镇政府驻地建成区总人口（万人）</td>	
			<td width="30%">${economy.zhu_ren}</td>
			<td width="20%" class="myInputTitle">乡镇政府驻地建成区总面积（万平方公里）</td>	
			<td width="30%">${economy.zhu_mian}</td>
	    </tr>
	    <tr>
			<td width="20%" class="myInputTitle">乡镇政府驻地建成区新增人口（万人）</td>	
			<td width="30%">${economy.zhu_xin}</td>
			<td width="20%" class="myInputTitle">乡镇转移到其它地方人口（万人）</td>	
			<td width="30%">${economy.zhu_zhuan}</td>
	    </tr>
	    <tr>
			<td width="20%" class="myInputTitle">职能类型</td>	
			<td width="30%">${economy.zhi_neng}</td>
			<td width="20%" class="myInputTitle">是否属于210个特色小镇</td>	
			<td width="30%">${economy.tese}</td>
	    </tr>
	    <tr>
			<td width="20%" class="myInputTitle">特色小镇职能类型</td>	
			<td width="30%">${economy.tese_zhineng}</td>
			<td width="20%" class="myInputTitle">乡镇空间位置地形</td>	
			<td width="30%">${economy.di_xing}</td>
	    </tr>
	    <tr>
			<td width="20%" class="myInputTitle">乡镇空间位置特殊区域（可多选）</td>	
			<td width="30%">${economy.te_shu}</td>
			<td width="20%" class="myInputTitle">城镇空间关系</td>	
			<td width="30%">${economy.kong_jian}</td>
	    </tr>
	    <tr>
			<td width="20%" class="myInputTitle">备注</td>	
			<td width="30%">${economy.remark}</td>
	</table>
</form>
</body>
</html>