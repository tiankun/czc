	}
	public Long getId() {
		return this.id;
	}
	public Long getParent() {
		return this.parent;
	}
	
	public void setParent(Long value) {
		this.parent = value;
	}
	
	public String getName() {
		return this.name;
	}
	
	public void setName(String value) {
		this.name = value;
	}
	
	public String getBiao_shi() {
		return this.biao_shi;
	}
	
	public void setBiao_shi(String value) {
		this.biao_shi = value;
	}
	
	public String getRemark() {
		return this.remark;
	}
	
	public void setRemark(String value) {
		this.remark = value;
	}
	
	public String getZhu_xiao() {
		return this.zhu_xiao;
	}
	
	public void setZhu_xiao(String value) {
		this.zhu_xiao = value;
	}
	
	public String getQuan_ming() {
		return this.quan_ming;
	}
	
	public void setQuan_ming(String value) {
		this.quan_ming = value;
	}
	
