package com.zsc.map;

public class Nitem_file{
	

	//数据库相关字段
	private Long id;    //主键
	private Long item_type;    //类型（1：供水；2：污水；3：垃圾）
	private Long yue_id;    //一水两污项目月报主键/项目主键
	private String path;    //文件存放位置
	private String pic_type;    //图片类型
	private String remark;    //图片说明
	private String jindu;    //进度类型
	private Long id_type;    //ID类型（1：项目；2：月报）
	private String file_name;    //文件名
	private String file_ext;    //文件扩展名
    //数据库相关字段 END

    /**
     * 构造函数
     */
	public Nitem_file(){
	}

	public Nitem_file(Long id){
		this.id = id;
	}

	
    //相关属性get/set方法
	public void setId(Long value) {
		this.id = value;
	}
	public Long getId() {
		return this.id;
	}

	public Long getItem_type() {
		return item_type;
	}
	
	public String getFile_name() {
		return file_name;
	}

	public void setFile_name(String file_name) {
		this.file_name = file_name;
	}

	public String getFile_ext() {
		return file_ext;
	}

	public void setFile_ext(String file_ext) {
		this.file_ext = file_ext;
	}

	public void setItem_type(Long item_type) {
		this.item_type = item_type;
	}

	public Long getYue_id() {
		return yue_id;
	}
	
	public Long getId_type() {
		return id_type;
	}

	public void setId_type(Long id_type) {
		this.id_type = id_type;
	}

	public void setYue_id(Long yue_id) {
		this.yue_id = yue_id;
	}

	public String getPath() {
		return path;
	}

	public void setPath(String path) {
		this.path = path;
	}

	public String getPic_type() {
		return pic_type;
	}

	public void setPic_type(String pic_type) {
		this.pic_type = pic_type;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getJindu() {
		return jindu;
	}

	public void setJindu(String jindu) {
		this.jindu = jindu;
	}
	
}
